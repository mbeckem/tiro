import std;

func fizzbuzz() {
    for (var i = 1; i <= 100; i = i + 1) {
        const message = if (i % 15 == 0) {
            "FizzBuzz";
        } else if (i % 3 == 0) {
            "Fizz";
        } else if (i % 5 == 0) {
            "Buzz";
        } else {
            "$i";
        };

        std.print(message);
    }
}
