#!/usr/bin/env python3

from tools import run_clang_format, run_cog

cog_files = [
    "src/tiro/mir/types.hpp",
    "src/tiro/mir/types.cpp",
    "src/tiro/mir/support.hpp",
    "src/tiro/mir/support.cpp",
]


def main():
    for file in cog_files:
        run_cog(file)
        run_clang_format(file)


if __name__ == "__main__":
    main()
